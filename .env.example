# Appwrite Configuration
# Get these values from your Appwrite Console (https://cloud.appwrite.io)

# Your Appwrite API Endpoint
NEXT_PUBLIC_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# Your Appwrite Project ID
NEXT_PUBLIC_APPWRITE_PROJECT_ID=your_project_id_here

# Optional: Database ID (if you're using Appwrite Database)
NEXT_PUBLIC_APPWRITE_DATABASE_ID=your_database_id_here

# Optional: Users Collection ID
NEXT_PUBLIC_APPWRITE_USERS_COLLECTION_ID=your_users_collection_id_here

# Wherebuy Module - Locations Collection ID
NEXT_PUBLIC_APPWRITE_LOCATIONS_COLLECTION_ID=your_locations_collection_id_here
